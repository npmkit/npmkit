{
  "name": "npmkit",
  "version": "0.3.2-alpha",
  "description": "Ultimate toolkit to control npm chaos",
  "main": "main.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test BABEL_DISABLE_CACHE=1 mocha --compilers js:babel-register --recursive --require ./tests/setup.js tests/unit/**/*.spec.js",
    "test-watch": "npm test -- --watch",
    "lint": "eslint app tests *.js",
    "hot-server": "node -r babel-register server.js",
    "build-main": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.electron.js --profile --colors",
    "build-renderer": "cross-env NODE_ENV=production node -r babel-register ./node_modules/webpack/bin/webpack --config webpack.production.js --profile --colors",
    "build": "npm run build-main && npm run build-renderer",
    "start": "cross-env NODE_ENV=production electron ./",
    "start-hot": "cross-env HOT=1 NODE_ENV=development electron -r babel-register ./app/main",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "dist-macos": "npm run build && build --macos",
    "dist-linux": "npm run build && build --linux",
    "dist-win": "npm run build && build --win --x64",
    "dist-all": "npm run build && build -mwl",
    "postinstall": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "docs-serve": "http-server ./docs",
    "docs-build": "esdoc -c ./.esdoc docs",
    "generate-icns": "nicns --in ./assets/npmkit-icon.png --out ./assets/icon.icns"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sergeybekrin/npmkit.git"
  },
  "bugs": {
    "url": "https://github.com/sergeybekrin/npmkit/issues"
  },
  "author": {
    "name": "Sergey Bekrin",
    "email": "sergey@bekrin.me"
  },
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.11.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-async-functions": "^6.3.13",
    "babel-plugin-transform-async-to-generator": "^6.4.0",
    "babel-plugin-webpack-loaders": "^0.4.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.7.2",
    "chromedriver": "^2.21.2",
    "co-mocha": "^1.1.2",
    "concurrently": "^2.2.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.0",
    "del": "^2.0.1",
    "devtron": "^1.2.1",
    "electron-builder": "^5.23.2",
    "electron-devtools-installer": "^1.1.4",
    "electron-mocha": "^2.2.0",
    "electron-prebuilt": "^1.0.3",
    "electron-rebuild": "^1.1.1",
    "esdoc": "^0.4.5",
    "esdoc-es7-plugin": "0.0.3",
    "eslint": "^2.10.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-jsx-a11y": "^1.2.0",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "fbjs-scripts": "^0.7.0",
    "file-loader": "^0.8.5",
    "http-server": "^0.9.0",
    "jsdom": "^9.0.0",
    "json-loader": "^0.5.4",
    "minimist": "^1.2.0",
    "mocha": "^2.3.4",
    "nock": "^8.0.0",
    "node-icns": "0.0.4",
    "node-libs-browser": "^1.0.0",
    "react-addons-perf": "^15.0.2",
    "react-addons-test-utils": "^15.0.2",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-inspector": "^0.5.1",
    "redux-mock-store": "^1.0.3",
    "selenium-webdriver": "^2.53.1",
    "sinon": "^1.17.2",
    "spectron": "^3.0.0",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.2",
    "stylus-loader": "^2.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-merge": "^0.14.1"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "electron-debug": "^1.0.1",
    "history": "^2.1.1",
    "lodash": "^4.13.1",
    "node-uuid": "^1.4.7",
    "normalize.css": "^4.1.1",
    "react": "^15.0.2",
    "react-addons-shallow-compare": "^15.0.2",
    "react-dom": "^15.0.1",
    "react-redux": "^4.4.2",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.2",
    "redux": "^3.5.2",
    "redux-actions": "^0.9.1",
    "redux-thunk": "^2.0.1",
    "semver": "^5.1.0",
    "strip-ansi": "^3.0.1",
    "thenby": "^1.1.3"
  },
  "devEngines": {
    "node": "6.x",
    "npm": "2.x || 3.x"
  },
  "build": {
    "appId": "org.npmkit.npmkit",
    "app-category-type": "public.app-category.developer-tools",
    "productName": "npmkit",
    "files": [
      "package.json",
      "main.js",
      "build/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "win": {
      "remoteReleases": true
    },
    "linux": {
      "target": [
		"AppImage",
		"deb"
      ]
    }
  },
  "directories": {
    "buildResources": "assets"
  }
}
